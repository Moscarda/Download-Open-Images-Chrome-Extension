{

	"manifest_version": 2,
	"name": "An Extension",
	"description": "I'm a description",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "titelino"
	},
	"icons":{
		"64": "icon.png"
	},
	"background": {
    "scripts": ["background.js"]
	},
	"permissions":[
	"activeTab",
	"http://*/*",
	"https://*/*",
	"tabs"
	]

}